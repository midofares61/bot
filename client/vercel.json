{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "pages/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "NEXT_PUBLIC_API_URL": "@next_public_api_url",
    "NEXT_PUBLIC_FACEBOOK_APP_ID": "@next_public_facebook_app_id",
    "NEXT_PUBLIC_CLIENT_URL": "@next_public_client_url",
    "NEXT_PUBLIC_FACEBOOK_API_VERSION": "@next_public_facebook_api_version"
  }
}
